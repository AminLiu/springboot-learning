<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.lybgeek.user.dao.UserDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.github.lybgeek.virtualcolumn.user.entity.User">
        <id column="id" property="id" />
        <result column="user_info" property="userInfo" />
        <result column="create_time" property="createTime" />
        <result column="v_user_name" property="username" />
        <result column="v_date_month" property="month" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_info, create_time,v_user_name,v_date_month
    </sql>

</mapper>
